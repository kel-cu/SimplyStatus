{
  "schemaVersion": 1,
  "id": "simplystatus",
  "version": "${version}",
  "name": "SimplyStatus",
  "description": "Simple, customizable Discord game status",
  "authors": [
    "Simply_Kel"
  ],
  "icon": "assets/simplystatus/logo.png",
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "main": [
      "og.__kel_.simplystatus.Main"
    ],
    "client": [
      "og.__kel_.simplystatus.client.MainClient",
      "og.__kel_.simplystatus.client.HotKeys"
    ],
    "server": [
      "og.__kel_.simplystatus.server.MainServer"
    ],
    "modmenu": [
      "og.__kel_.simplystatus.configs.ModMenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.8",
    "minecraft": "^1.19"
  },
  "conflicts": {},
  "breaks": {
    "amrpcf": "*"
  },
  "contact": {
    "issues": "https://github.com/simply-kel/SimplyStatus-fabric/issues",
    "homepage": "https://modrinth.com/mod/simplystatus"
  },
  "mixins": [
    "simplystatus.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.simplystatus.github": "https://github.com/simply-kel/SimplyStatus-fabric",
        "modmenu.simplystatus.curseforge": "https://www.curseforge.com/minecraft/mc-mods/simplystatus",
        "modmenu.simplystatus.modrinth": "https://modrinth.com/mod/simplystatus"
      }
    }
  }
}
